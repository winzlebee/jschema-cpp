{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://verbotics.com/schemas/model.schema.json",
    "title": "A test schema",
    "description": "Schema that describes a test object",
    "type": "object",
    "properties" : {
        "name" : {
            "type" : "string"
        },
        "isHome" : {
            "type" : "boolean",
            "default" : false
        },
        "address" : {
            "type" : "object",
            "properties" : {
                "streetNumber" : {
                    "type" : "integer"
                },
                "streetType" : {
                    "type" : "string",
                    "enum" : ["street", "lane", "crescent"]
                }
            }
        }
    }
}